<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Link has been Expired</title>
    <!-- fonts -->
    <link
      href="https://fonts.googleapis.com/css2?family=Roboto:wght@300&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://cdnjs.cloudflare.com/ajax/libs/tailwindcss/2.2.19/tailwind.min.css"
      rel="stylesheet"
    />
    <link
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css"
      rel="stylesheet"
    />
  </head>
  <style>
    * { margin: 0; padding: 0; } body { width: 100%; height: 100%;
    background-color: rgb(245, 245, 245); } .container { width: 80%; margin: 0
    auto; } .wrapper { display: flex; align-items: center; flex-direction:
    column; justify-content: center; width: 100%; height: 100vh; } i { color:
    #bfd1f1; font-size: 3em; } .card { max-width: auto; box-shadow: 0 2px 3px
    rgba(0, 0, 0, 0.19), 0 0.5px 0.5px rgba(0, 0, 0, 0.23); } .toggler {
    font-size: 0.6em; color: #9ca3af; } #alert { opacity: 0; } .text { color:
